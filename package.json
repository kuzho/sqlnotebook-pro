{
  "name": "sqlnotebook-pro",
  "displayName": "SQL Notebook Pro",
  "publisher": "kuzho",
  "description": "View SQL files as notebooks. Execute cells and view query output. (Custom Build)",
  "icon": "media/logo128.png",
  "repository": {
    "url": "https://github.com/kuzho/sqlnotebook-pro.git"
  },
  "version": "1.0.1",
  "preview": false,
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Data Science",
    "Notebooks",
    "Visualization"
  ],
  "activationEvents": [],
  "main": "./dist/index.js",
  "capabilities": {
    "virtualWorkspaces": true,
    "untrustedWorkspaces": {
      "supported": false
    }
  },
  "extensionKind": [
    "workspace"
  ],
  "contributes": {
    "notebooks": [
      {
        "type": "sql-notebook",
        "displayName": "SQL Notebook",
        "priority": "option",
        "selector": [
          {
            "filenamePattern": "*.sql"
          }
        ]
      }
    ],
    "notebookRenderer": [
      {
        "id": "sql-notebook-renderer",
        "displayName": "Interactive SQL Table",
        "entrypoint": "./dist/renderer.js",
        "mimeTypes": [
          "application/vnd.code-sql-notebook.table+json"
        ]
      }
    ],
    "menus": {
      "view/item/context": [
        {
          "command": "sqlnotebook.editConnection",
          "when": "view == sqlnotebook-connections && viewItem == database",
          "group": "1_modification"
        },
        {
          "command": "sqlnotebook.deleteConnectionConfiguration",
          "when": "view == sqlnotebook-connections && viewItem == database",
          "group": "1_modification"
        }
      ],
      "view/title": []
    },
    "commands": [
      {
        "command": "sqlnotebook.editConnection",
        "title": "Edit Connection",
        "icon": "$(edit)"
      },
      {
        "command": "sqlnotebook.deleteConnectionConfiguration",
        "title": "Delete SQL Connection Configuration"
      },
      {
        "command": "sqlnotebook.refreshConnectionPanel",
        "title": "Refresh SQL Connection Panel",
        "shortTitle": "Refresh"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "sqlnotebook",
          "title": "SQL Notebook",
          "icon": "media/logo.svg"
        }
      ]
    },
    "configuration": {
      "title": "SQL Notebook",
      "properties": {
        "sqlnotebook.connections": {
          "type": "array",
          "default": [],
          "description": "List of saved database connection configurations.",
          "scope": "window"
        },
        "SQLNotebook.useLanguageServer": {
          "type": "boolean",
          "default": false,
          "description": "(Unstable) Use embedded language server for intelligent completion and hover information."
        },
        "SQLNotebook.outputJSON": {
          "type": "boolean",
          "default": false,
          "description": "Output JSON in addition to markdown. Other extensions may use this output type to render an interactive table."
        },
        "SQLNotebook.queryTimeout": {
          "type": "number",
          "default": 30000,
          "description": "Query timeout in milliseconds for cell query execution."
        },
        "SQLNotebook.maxResultRows": {
          "type": "number",
          "default": 25,
          "description": "Maximum number of result rows to display before truncating result table."
        }
      }
    },
    "views": {
      "sqlnotebook": [
        {
          "id": "sqlnotebook-connections",
          "name": "SQL Connections",
          "visibility": "visible",
          "icon": "media/logo.svg",
          "contextualTitle": "Connections"
        },
        {
          "type": "webview",
          "id": "sqlnotebook.connectionForm",
          "name": "New SQL Connection",
          "contextualTitle": "New Connection",
          "visibility": "visible"
        },
        {
          "id": "sqlnotebook-helpfeedback",
          "name": "Help and Feedback",
          "visibility": "collapsed",
          "icon": "media/logo.svg",
          "contextualTitle": "Help and Feedback"
        }
      ]
    }
  },
  "scripts": {
    "build": "npm run compile && npx @vscode/vsce package",
    "vscode:prepublish": "npm run compile",
    "compile": "npm run build:server && npm run build:webview && npm run build:renderer && shx mkdir -p dist/node_modules && shx cp -r node_modules/sql.js dist/node_modules",
    "build:server": "esbuild ./src/main.ts --sourcemap --minify --bundle --platform=node --external:vscode --external:pg-native --outfile=./dist/index.js",
    "build:webview": "esbuild ./webview/main.tsx --sourcemap --minify --bundle --outfile=./dist/webview/main-bundle.js",
    "build:renderer": "esbuild ./src/renderer.tsx --bundle --outfile=./dist/renderer.js --format=esm --platform=browser",
    "clean": "rm -rf ./dist",
    "lint": "eslint src --ext ts",
    "watch": "npm run build:server -- --watch & npm run build:webview -- --watch & npm run build:renderer -- --watch",
    "fmt": "prettier --write --ignore-path .gitignore ."
  },
  "devDependencies": {
    "@types/escape-html": "^1.0.1",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/mssql": "^7.1.3",
    "@types/node": "^18.0.0",
    "@types/pg": "^8.6.1",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/sql.js": "^1.4.3",
    "@types/vscode": "^1.75.0",
    "@types/vscode-notebook-renderer": "^1.57.8",
    "@types/vscode-webview": "^1.57.0",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "@vscode/vsce": "^3.2.0",
    "esbuild": "^0.17.19",
    "eslint": "^8.20.0",
    "glob": "^7.1.7",
    "ovsx": "^0.8.0",
    "prettier": "^2.6.0",
    "shx": "^0.3.4",
    "typescript": "^5.0.4",
    "vscode-notebook-error-overlay": "^1.0.1",
    "vscode-test": "^1.6.1"
  },
  "dependencies": {
    "@tanstack/react-table": "^8.21.3",
    "@vscode/webview-ui-toolkit": "^0.9.3",
    "icons-react": "^3.7.1",
    "mssql": "^7.2.1",
    "mysql2": "^2.3.0",
    "pg": "^8.7.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sql.js": "^1.7.0",
    "vscode-languageclient": "^7.0.0",
    "xlsx": "^0.18.5"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true
  }
}